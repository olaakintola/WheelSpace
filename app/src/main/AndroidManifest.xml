<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wheelspace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WheelSpace">
        <activity android:name=".OffPeakResultActivity"></activity>
        <activity android:name=".RealtimeResultActivity" />
        <activity
            android:name=".ForumActivity"
            android:label="@string/activity_forum" />
        <activity
            android:name=".PlanMyJourneyActivity"
            android:label="@string/activity_plan_my_journey" />
        <activity
            android:name=".RealtimeAccessActivity"
            android:label="@string/activity_realtime_access" />
        <activity
            android:name=".MyStatusActivity"
            android:label="@string/activity_my_status" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>